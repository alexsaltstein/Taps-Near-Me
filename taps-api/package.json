{
  "name": "taps-api",
  "version": "1.0.0",
  "description": "This is the api repo for taps near me",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "seedBeers": "node ./tasks/seedBeers.js",
    "seedVenues": "node ./tasks/seedVenues.js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.mjs$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "src/node_modules/(?!@sapper)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "svelte"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/node_modules"
    ]
  },
  "author": "Taps Near Me",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^4.1.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@testing-library/react": "^12.1.2",
    "babel": "^6.23.0",
    "babel-jest": "^27.2.5",
    "jest": "^27.2.5",
    "jest-dom": "^4.0.0"
  }
}
